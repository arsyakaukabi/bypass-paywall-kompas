{
  "manifest_version": 3,
  "name": "Bypass Paywall Kompas",
  "version": "1.0",
  "description": "Remove paywalls on kompas.id",
  "permissions": ["tabs","activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.kompas.id/*"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
